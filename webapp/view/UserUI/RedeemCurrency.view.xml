	xmlns:l="sap.ui.layout"
	xmlns="sap.m"
	height="100%">
	<l:VerticalLayout
		class="sapUiContentPadding"
		width="100%">
		<l:content>
			<HBox
					alignItems="End"
					justifyContent="Center">
				<core:Icon
